    "scoring_endpoints": {
      "score_basic": {
        "endpoint": "POST /api/score",
        "description": "Basic scoring request",
        "request": {
          "transcription": "I really enjoy traveling because it allows me to experience different cultures and meet new people. Last year, I visited Japan and it was an amazing experience. The food was delicious and the people were very friendly.",
          "topic": "Travel",
          "level": "intermediate"
        }
      },
      "score_with_question": {
        "endpoint": "POST /api/score",
        "description": "Scoring with question text for relevance checking",
        "request": {
          "transcription": "I like music very much. I listen to pop songs every day. Music makes me happy and relaxed.",
          "questionText": "What kind of music do you like?",
          "topic": "Music",
          "level": "beginner"
        }
      },
      "score_advanced": {
        "endpoint": "POST /api/score",
        "description": "Advanced level scoring",
        "request": {
          "transcription": "I'm particularly fond of classical music, especially compositions from the Romantic period. I find that listening to works by composers like Chopin and Debussy helps me concentrate when I'm studying. The intricate melodies and harmonies create an atmosphere that's both stimulating and calming.",
          "questionText": "What type of music do you enjoy listening to?",
          "topic": "Music",
          "level": "advanced"
        }
      },
      "score_off_topic": {
        "endpoint": "POST /api/score",
        "description": "Test off-topic response (should be penalized)",
        "request": {
          "transcription": "I love playing football and watching sports on TV. My favorite team is Manchester United.",
          "questionText": "What kind of music do you like?",
          "topic": "Music",
          "level": "intermediate"
        }
      }
    },
    "chat_endpoint": {
      "chat_basic": {
        "endpoint": "POST /api/chat",
        "description": "Basic chat request",
        "request": {
          "model": "llama3.1:latest",
