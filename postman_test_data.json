  "quick_test_scenarios": {
    "scenario_1_complete_flow": {
      "description": "Complete flow: Generate topic -> Generate question -> Generate answer -> Score response",
      "steps": [
        {
          "step": 1,
          "action": "Generate Part 1 topics",
          "endpoint": "POST /api/generate/topics",
          "request": {
            "partNumber": 1,
            "difficultyLevel": "intermediate",
            "count": 3,
            "topicCategory": "hobbies"
          }
        },
        {
          "step": 2,
          "action": "Generate Part 2 question",
          "endpoint": "POST /api/generate/questions",
          "request": {
            "partNumber": 2,
            "difficultyLevel": "intermediate",
            "topic": "Hobbies"
          }
        },
        {
          "step": 3,
          "action": "Generate sample answer",
          "endpoint": "POST /api/generate/answers",
          "request": {
            "question": "Describe a hobby you enjoy",
            "partNumber": 2,
            "targetBand": 7.0
          }
        },
        {
          "step": 4,
          "action": "Score the generated answer",
          "endpoint": "POST /api/score",
          "request": {
            "transcription": "I'd like to talk about my hobby of photography. I've been interested in photography for about three years now, and I find it incredibly rewarding. What I enjoy most is capturing moments and seeing the world from different perspectives through my camera lens.",
            "questionText": "Describe a hobby you enjoy",
            "topic": "Hobbies",
            "level": "intermediate"
          }
        }
      ]
    },
